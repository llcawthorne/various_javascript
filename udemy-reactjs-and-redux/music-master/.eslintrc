{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": "warn",
    "no-unused-vars": [
      "error", 
      {
        "vars": "all", 
        "args": "none",
      }
    ],
    "react/jsx-filename-extension": [
      1, 
      { 
        "extensions": [
          ".js", 
          ".jsx"
        ]
      }
    ],
  }
}
